<%- contentFor('content') %>
    <div class="page-wrapper">
        <div class="page-content--bge5">
            <div class="container">
                <div class="login-wrap">
                    <div class="login-content">
                        <div class="login-logo">
                            <a href="#">
                                <h3>Edit Productfee</h3>
                            </a>
                        </div>

                        <div class="login-form">
                            <form action="/products/edit-productfee/<%= product._id %>" method="post" id="test-form">
                                <div class="form-group">
                                        <label>Unit Price</label>
                                        <input value="<%= product.Principal %>" class="au-input au-input--full" type="text" name="productprice" placeholder="Unit Price">
                                    </div>
                                    <div class= "form-group">
                                        <label>Shipping Charges</label>
                                        <input value=" <%= product.ShippingChargeCurrency %><%= product.ShippingCharge %>" class="au-input au-input--full" type="text" name="ShippingCharge" placeholder="ShippingCharges Fee">
                                    </div>
                                    <div class="form-group">
                                            <label>FBA Fee</label>
                                            <input value="<%= product.FBA %>" class="au-input au-input--full" type="text" name="fbafee" placeholder="FBA Fee" disabled >
                                        </div>

                                        <div class="form-group">
                                                <label>Referral Fee</label>
                                                <input value="<%= product.ReferralFee %>" class="au-input au-input--full" type="text" name="referralfee" placeholder="Referral Fee" disabled>
                                            </div>
                                                                      
                                              
                                        <div class="form-group">
                                                <label>Amazon Fee</label>
                                                <input value="<%= product.amznFeePerOrder %>" class="au-input au-input--full" type="text" name="amazonfee" placeholder="Amazon Fee" disabled >
                                            </div>   

                                

                        </div>
                        <button class="au-btn au-btn--block au-btn--green m-b-20" type="submit">Submit</button>
                    </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    </div>
    </div>